<div class="container">
	<table class="table table-hover">
		<thead>
			<th scope="col">Documento de Identidad</th>
			<th scope="col">Nombre</th>
			<th scope="col">Correo</th>
			<th scope="col">Telefono</th>
			<th scope="col"></th>
		</thead>
		<tbody>
			{{#each (mostrarAspirantesXCurso id)}}
				<tr>
					<form action="/eliminar?rol={{../rol}}" method="post">
						<input type="hidden" name="id_curso" value="{{../id}}">
						<input type="hidden" name="docAspirante" value="{{doc}}">
    					<td>{{doc}}</td>
    					<td>{{nombre}}</td>
    					<td>{{correo}}</td>
    					<td>{{telefono}}</td>
    					<td><button type="submit" class="btn btn-primary" name="boton" value="eliminar">Eliminar</button></td>
					</form>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>